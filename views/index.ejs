<html>
  <head>
    <title>Sql Unit Testing Suite</title>
  </head>
  <body>
    <h1 style="margin-bottom: 0;float:left;">SQL Unit Tests </h1>
    <a href="test/new">
      <button style="margin-bottom:0;margin-top:10px;float:right;">Add New +</button>
    </a>
    <hr style="clear:both;"/>
    <% if(tests && tests[0]){ %>
      <ul>
        <% for(var i = 0; i < tests.length; i++) { %>
          <li>
            <a href="test/<%= tests[i] %>">
              <%= tests[i] %>
            </a>
          </li>
        <% } %>
      </ul>
    <% } else { %>
      <p>No Tests have been created! Create a <a href="test/new">new</a> one?</p>
    <% } %>
  </body>
</html>
